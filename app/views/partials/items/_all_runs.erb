<% @runs.each do |run| %>
  <% if run.winner.nil? && run.item.name != "" %>
    <tr>
      <td class="bg-primary"><b><%= run.id.to_s %></b></td>
      <td class="bg-info"><%= run.boss.name %></td>
      <td class="bg-success"><%= run.item.name.capitalize %></td>
      <td class="bg-danger">Unclaimed</td>
      <td class="bg-info"><%= run.date.to_s %></td>
      <td class="bg-info"><%= run.time.to_s %></td>
    </tr>
  <% end %>
<% end %>
